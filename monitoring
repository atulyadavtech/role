


wget https://dl.grafana.com/oss/release/grafana_7.3.1_amd64.deb
systemctl enable grafana-server
systemctl restart  grafana-server (http://ipserver:3000)
apt-get install -f


wget https://github.com/prometheus/node_exporter/releases/download/v1.0.0/node_exporter-1.0.0.linux-amd64.tar.gz
cd node_exporter-1.0.0.linux-amd64/
mv node_exporter /usr/local/bin/;chown node_exporter:node_exporter /usr/local/bin/node_exporter
